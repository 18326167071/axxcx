/* pages/checks/checks.wxss */
.checks-box{
  border-bottom: 20rpx solid #f5f8fa;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.checks-box .checks-list{
  padding: 30rpx 0;
  border-bottom: 2rpx solid #e9e9e9;
  overflow: hidden;
}
.checks-box .checks-list:last-child{
  border: none;
}
.checks-box .checks-list>.key{
  float: left;
  font-size: 28rpx;
  color: #999;
}
.checks-box .checks-list>.val{
  float: right;
  font-size: 28rpx;
  color: #666;
  width: 60%;
  word-break:break-all;
  text-align: right;
}