/* company/search/search.wxss */
.company-search{
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  overflow-x: hidden;
  overflow-y: auto;
}
.all-input{
  font-size: 28rpx;
  color: #ccc;
  line-height: 80rpx;
}
/* 企业地区 */
.company-search>.search-area{
  padding: 40rpx 30rpx;
  border-top: 20rpx solid #f5f8fa;
}
.company-search>.search-area>.title{
  overflow: hidden;
}
.company-search>.search-area>.title>image{
  display: block;
  width: 22rpx;
  height: 28rpx;
  margin-right: 14rpx;
  float: left;
  margin-top: 2rpx;
}
.company-search>.search-area>.title>text{
  font-size: 32rpx;
  color: #333;
  line-height: 32rpx;
  display: block;
  float: left;
}
.company-search>.search-area>.content{
  margin-top: 36rpx;
}
.company-search>.search-area>.content>picker{
  height: 80rpx;
  display: block;
  position: relative;
}
.company-search>.search-area>.content .picker{
  height: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  display: flex;
  justify-content: space-around;
}
.company-search>.search-area>.content .picker>.area{
  width: 100%;
  height: 80rpx;
  border: 2rpx solid #e9e9e9;
  border-radius: 4rpx;
  display: block;
  position: relative;
  font-size: 28rpx;
  line-height: 76rpx;
  color: #666;
  padding-left: 20rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.company-search>.search-area>.content .picker>.area-line{
  width: 24rpx;
  color: #e9e9e9;
  line-height: 80rpx;
}
.arrow{
  display: block;
  width: 18rpx;
  height: 12rpx;
  position: absolute;
  right: 20rpx;
  top: 34rpx;
}
/* 企业资质 */
.company-search>.search-aptitude{
  padding: 40rpx 30rpx;
  border-top: 20rpx solid #f5f8fa;
}
.company-search>.search-aptitude>.title{
  overflow: hidden;
}
.company-search>.search-aptitude>.title>image{
  display: block;
  width: 24rpx;
  height: 32rpx;
  margin-right: 12rpx;
  float: left;
}
.company-search>.search-aptitude>.title>text{
  font-size: 32rpx;
  color: #333;
  line-height: 32rpx;
  display: block;
  float: left;
}
.company-search>.search-aptitude>.content{
  overflow: hidden;
}
.company-search>.search-aptitude>.content>picker{
  width: 320rpx;
  height: 80rpx;
  box-sizing: border-box;
  border-radius: 4rpx;
  border: 2rpx solid #e9e9e9;
  color: #333;
  font-size: 28rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  position: relative;
  margin-top: 40rpx;
  padding-right: 40rpx;
}
.company-search>.search-aptitude>.content .picker{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #666;
}
.company-search>.search-aptitude>.content>picker:nth-child(2n){
  float: right;
}
.company-search>.search-aptitude>.content>picker:nth-child(2n+1){
  float: left;
}
.company-search>.search-aptitude>.footer{
  margin-top: 40rpx;
  overflow: hidden;
}
.company-search>.search-aptitude>.footer>.add{
  width: 210rpx;
  height: 70rpx;
  border-radius: 4rpx;
  background-color: #44c98b;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 70rpx;
  float: left;
}
.company-search>.search-aptitude>.footer>.look{
  float: right;
  line-height: 70rpx;
  color: #44c98b;
  margin-left: 30rpx;
}
.company-search>.search-aptitude>.footer>.look>image{
  display: block;
  float: left;
  width: 28rpx;
  height: 20rpx;
  margin-top: 25rpx;
  margin-right: 12rpx;
}
.company-search>.search-aptitude>.footer>.look>text{
  float: left;
  display: block;
  font-size: 32rpx;
}
.company-search>.search-aptitude>.footer>.num{
  color: #333;
  font-size: 28rpx;
  line-height: 70rpx;
  float: right;
}
.company-search>.search-aptitude>.footer>.num>text{
  font-size: 32rpx;
  color: #44c98b;
  font-weight: bold;
  margin: 0 12rpx;
  display: inline-block;
}
