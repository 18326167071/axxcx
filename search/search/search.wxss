  padding: 40rpx 30rpx;
  border-top: 20rpx solid #f5f8fa;
}
.company-search>.search-other>.title{
  overflow: hidden;
}
.company-search>.search-other>.title>image.time{
  display: block;
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
  margin-top: 3rpx;
  float: left;
}
.company-search>.search-other>.title>image.money{
  display: block;
  width: 24rpx;
  height: 32rpx;
  margin-right: 12rpx;
  float: left;
}
.company-search>.search-other>.title>text{
  font-size: 32rpx;
  color: #333;
  line-height: 32rpx;
  display: block;
  float: left;
}
.company-search>.search-other>.title>.fr{
  float: right;
  color: #999;
  font-size: 26rpx;
}
.company-search>.search-other>.time{
  margin-top: 36rpx;
  border: 2rpx solid #e9e9e9;
  height: 80rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  border-radius: 4rpx;
}
.company-search>.search-other>.time>picker{
  width: 45%;
  height: 80rpx;
  display: block;
  position: relative;
}
.company-search>.search-other>.time>.line{
  width: 24rpx;
  height: 1rpx;
  background-color: #ccc;
  margin-top: 40rpx;
}
.company-search>.search-other>.time .picker{
  height: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
}
.company-search>.search-other>.time .picker>.area{
  color: #999;
  font-size: 28rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.company-search>.search-other>.time .picker>.area>text{
  color: #666;
  margin-left: 10rpx;
}
