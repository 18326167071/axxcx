  top: 300rpx;
  position: absolute;
  left: 105rpx;
  box-sizing: border-box;
}
.done-vip-box>view{
  font-size: 32rpx;
  text-align: center;
  color: #333;
  line-height: 1.5;
}
.done-vip-box>.close-btn{
  display: block;
  width: 46rpx;
  height: 46rpx;
