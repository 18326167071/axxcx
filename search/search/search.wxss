/* 开始查询 */
.company-search>.sure{
  border-top: 20rpx solid #f5f8fa;
}
.company-search>.sure>view{
  width: 650rpx;
  height: 92rpx;
  border-radius: 4rpx;
  background-color: #44c98b;
  font-size: 32rpx;
  color: #fff;
  line-height: 92rpx;
  text-align: center;
  margin: 80rpx auto;
}
/* 已选条件 */
.company-search>.choose{
  width: 100vw;
  height: 100vh;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0,0,0,.5);
  z-index: 10000;
}
.company-search>.choose>.choose-box{
  width: 600rpx;
  padding: 0 0 40rpx;
  background-color: #fff;
  margin: 80rpx auto;
  border-radius: 8rpx;
  position: relative;
}
.company-search>.choose>.choose-box>.title{
  height: 100rpx;
  border-bottom: 2rpx solid #e9e9e9;
  line-height: 100rpx;
  color: #333;
  font-size: 32rpx;
  text-align: center;
}
.company-search>.choose>.choose-box>.content>view{
  margin-top: 40rpx;
}
.company-search>.choose>.choose-box>.content>view>.top{
  overflow: hidden;
}
.company-search>.choose>.choose-box>.content>view>.top>.left{
  float: left;
  width: 164rpx;
  height: 56rpx;
  border-radius: 4rpx;
  background-color: #44c98b;
  margin-left: 30rpx;
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  line-height: 56rpx;
}
.company-search>.choose>.choose-box>.content>view>.top>.right{
  float: right;
  margin-right: 30rpx;
}
.company-search>.choose>.choose-box>.content>view>.top>.right>image{
  display: block;
  width: 25rpx;
  height: 28rpx;
  float: left;
  margin: 14rpx 12rpx;
}
.company-search>.choose>.choose-box>.content>view>.top>.right>view{
  font-size: 28rpx;
  line-height: 56rpx;
  color: #ef6e50;
  float: left;
}
.company-search>.choose>.choose-box>.content>view>.bottom{
  overflow: hidden;
}
.company-search>.choose>.choose-box>.content>view>.bottom>view:nth-child(2n+1){
  float: left;
  font-size: 28rpx;
  color: #666;
  line-height: 1.2;
  margin-top: 30rpx;
  margin-left: 30rpx;
  width: calc(50% - 30rpx);
}
.company-search>.choose>.choose-box>.content>view>.bottom>view:nth-child(2n){
  float: right;
  font-size: 28rpx;
  color: #666;
  line-height: 1.2;
  margin-top: 30rpx;
  margin-right: 30rpx;
  width: calc(50% - 30rpx);
  text-align: right;
}
.company-search>.choose>.choose-box>.close{
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  bottom: -78rpx;
  left: 50%;
  margin-left: -24rpx;
  display: block;
}
.company-search>.choose>.choose-box>.close>image{
  width: 100%;
  height: 100%;
  display: block;
}
/* 授权弹窗 */
.container{
  width: 100%;
}
.container.popup{
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,.4);
}
.popup-box{
  width: 600rpx;
  height: 392rpx;
  margin: 0 auto;
  margin-top: 40%;
  border-radius: 8rpx;
  overflow: hidden;
  background-color: #fff;
}
.popup-box .box-title{
  line-height: 1;
  font-size: 36rpx;
  padding-top: 60rpx;
  color: #333;
  position: relative;
  text-align: center;
  padding-bottom: 10rpx;
}
.popup-box .box-title>image{
  display: block;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}
.popup-box>.btn,.popup-box>button{
  width: 500rpx;
  height: 80rpx;
  box-sizing: border-box;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
  line-height: 80rpx;
  font-size: 28rpx;
}
.popup-box>button{
  background-color: #51a938;
  border-color: #51a938;
  color: #fff;
}
.popup-box>.btn{
  border: 2rpx solid #eaecee;
  background-color: #f8f8f8;
  overflow: hidden;
  color: #666;
}
.popup-box>button>image{
  display: block;
  width: 40rpx;
  height: 32rpx;
  margin: 24rpx 20rpx;
}
.popup-box>.btn>image{
  display: block;
  width: 38rpx;
  height: 38rpx;
  margin: 21rpx 20rpx;
}
/* 会员卡弹窗 */
.vip{
  width: 100%;
}
.vip.popup{
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0,0,0,.4);
}
.vip-box{
  width: 574rpx;
  height: 626rpx;
  margin: 0 auto;
  margin-top: 20%;
  position: relative;
}
.vip-box>image{
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.vip-box>.card-btn{
  width: 430rpx;
  height: 90rpx;
  background-color: #44c98b;
  border-radius: 4rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 90rpx;
  text-align: center;
  position: absolute;
  bottom: 60rpx;
  left: 72rpx;
  letter-spacing: 2rpx;
}
.vip-box>.close-btn{
  display: block;
  width: 46rpx;
  height: 46rpx;
  position: absolute;
  left: 264rpx;
  top: 686rpx;
}
/* 会员卡弹窗 */
.done-vip{
  width: 100%;
}
.done-vip.popup{
